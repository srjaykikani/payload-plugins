{
  "name": "@jhb.software/payload-seo-plugin",
  "version": "0.1.4",
  "description": "SEO plugin for Payload CMS with AI-powered meta descriptions, keyword tracking and content analysis",
  "bugs": "https://github.com/jhb-software/payload-plugins/issues",
  "repository": "https://github.com/jhb-software/payload-plugins",
  "keywords": [
    "payload",
    "plugin",
    "seo"
  ],
  "author": "JHB Software",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "format": "prettier --write src",
    "test": "jest",
    "lint": "eslint .",
    "prepublish": "tsc"
  },
  "dependencies": {
    "openai": "^6.8.1"
  },
  "peerDependencies": {
    "@payloadcms/plugin-seo": "^3.5.0",
    "@payloadcms/richtext-lexical": "^3.5.0",
    "@payloadcms/ui": "^3.5.0",
    "next": "15.5.6",
    "payload": "^3.5.0",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "prettier": "^3.6.2",
    "typescript": "5.9.3"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "main": "./dist/index.js",
    "registry": "https://registry.npmjs.org/",
    "types": "./dist/index.d.ts",
    "access": "public",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./client": {
        "import": "./dist/exports/client.js",
        "types": "./dist/exports/client.d.ts",
        "default": "./dist/exports/client.js"
      }
    }
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "default": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./client": {
      "import": "./src/exports/client.ts",
      "default": "./src/exports/client.ts",
      "types": "./src/exports/client.ts"
    }
  }
}
