    "next": "15.5.6",
    "payload": "^3.60.0",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "typescript": "5.9.3"
  }
