{
  "name": "@jhb.software/payload-cloudinary-plugin",
  "version": "0.2.2",
  "description": "Payload CMS plugin that adds Cloudinary image upload and management capabilities to media collections.",
  "bugs": "https://github.com/jhb-software/payload-plugins/issues",
  "repository": "https://github.com/jhb-software/payload-plugins",
  "keywords": [
    "payload",
    "plugin",
    "cloudinary"
  ],
  "author": "JHB Software",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "format": "prettier --write src",
    "test": "jest",
    "lint": "eslint .",
    "prepublish": "tsc"
  },
  "dependencies": {
    "cloudinary": "^2.7.0"
  },
  "peerDependencies": {
    "payload": "^3.59.1"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "typescript": "5.9.3"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "main": "./dist/index.js",
    "registry": "https://registry.npmjs.org/",
    "types": "./dist/index.d.ts",
    "access": "public",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "default": "./src/index.ts",
      "types": "./src/index.ts"
    }
  }
}
